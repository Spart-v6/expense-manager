{
  "expo": {
    "name": "Thrifty",
    "slug": "thrifty-expense-manager",
    "version": "2.2.1",
    "orientation": "portrait",
    "icon": "./assets/newIcons/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/newIcons/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0B0B0B"
    },
    "notification": {
      "icon": "./assets/newIcons/notify-ico.png",
      "androidMode": "default",
      "androidCollapsedTitle": "Expense Reminder"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/newIcons/notify-ico.png",
          "color": "#0B0B0B",
          "defaultChannel": "default"
        }
      ],
      [
        "react-native-permissions",
        {
          "iosPermissions": [
            "Camera",
            "Microphone"
          ]
        }
      ],
      "expo-asset",
      "expo-font"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "4"
    },
    "android": {
      "package": "com.ankur24.expensemanager",
      "versionCode": 22,
      "adaptiveIcon": {
        "foregroundImage": "./assets/newIcons/adaptive-icon.png",
        "backgroundColor": "#0B0B0B"
      },
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "NOTIFICATIONS",
        "android.permission.READ_SMS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "56ae8d30-a5dd-4454-a366-9cf2fe5d6eac"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/56ae8d30-a5dd-4454-a366-9cf2fe5d6eac"
    }
  }
}